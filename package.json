{
    "name": "elevation-service",
    "type": "module",
    "private": true,
    "version": "2.0.0",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "npm run build; concurrently \"npm run dev:watch\" \"npm run dev:start\"",
        "dev:watch": "tsc --watch",
        "dev:start": "nodemon --watch 'dist' --exec 'npm start'",
        "build": "tsc",
        "test": "jest"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.550.0",
        "@fastify/cors": "^9.0.1",
        "@sindresorhus/fnv1a": "^3.1.0",
        "@turf/meta": "^6.5.0",
        "dotenv": "^16.4.5",
        "fastify": "^4.26.2",
        "lru-cache": "^10.2.0"
    },
    "devDependencies": {
        "@types/geojson": "^7946.0.14",
        "@types/node": "^20.12.5",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^27.9.0",
        "globals": "^15.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "typescript": "^5.4.4"
    }
}
