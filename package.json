{
    "name": "elevation-service",
    "packageManager": "pnpm@9.1.0",
    "type": "module",
    "private": true,
    "version": "3.0.0",
    "scripts": {
        "start": "node --require dd-trace/init dist/server.js",
        "dev": "pnpm build; concurrently \"pnpm:dev:*\"",
        "dev:watch": "tsc --watch",
        "dev:start": "nodemon --watch 'dist' --exec 'NODE_ENV=development pnpm start'",
        "build": "tsc",
        "test": "NODE_ENV=test node --test --test-concurrency=1",
        "test:ci": "pnpm install && pnpm build && pnpm test"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.588.0",
        "@fastify/compress": "^7.0.3",
        "@fastify/cors": "^9.0.1",
        "@mapbox/polyline": "^1.2.1",
        "@sindresorhus/fnv1a": "^3.1.0",
        "@turf/meta": "^6.5.0",
        "dd-trace": "^5.14.1",
        "dotenv": "^16.4.5",
        "fastify": "^4.27.0",
        "geobuf": "^3.0.2",
        "lru-cache": "^10.2.2",
        "pbf": "^3.2.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@types/geobuf": "^3.0.4",
        "@types/geojson": "^7946.0.14",
        "@types/mapbox__polyline": "^1.0.5",
        "@types/node": "^20.14.1",
        "@types/pbf": "^3.0.5",
        "concurrently": "^8.2.2",
        "eslint": "^9.4.0",
        "geojson": "^0.5.0",
        "globals": "^15.3.0",
        "nodemon": "^3.1.3",
        "prettier": "^3.3.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0"
    }
}
